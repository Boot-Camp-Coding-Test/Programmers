      "source": [
        "def solution(n):\n",
        "    answer = 0\n",
        "    res = n ** 0.5\n",
        "    if str(res)[-1] == '0':\n",
        "        answer = (res+1)**2\n",
        "\n",
        "    else:\n",
        "        answer = -1\n",
        "    return answer"
      ],
      "execution_count": 36,
      "outputs": []
