        "def solution(n):\n",
        "    n = str(n)\n",
        "    answer =[]\n",
        "    if len(n) == 1 :\n",
        "      return [int(n)]\n",
        "    answer.append(int(n[-1]))\n",
        "    return answer + solution(int(n[:-1]))"
